

@{
    ViewBag.Title = "layout";
    if (Session["USERNAME"] == null)
    {
        Response.Redirect("~/Home/Login");
    }

}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title> Đại Lý | Trang chủ</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link href="~/Content/download/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link href="~/Content/download/dist/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Content/self-writing/Layout-CSS.css" rel="stylesheet" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <script src="~/Content/download/angularjs/angular.min.js"></script>
    <script src="~/Content/self-writing/js/app.js"></script>
    <script src="~/Content/self-writing/js/Service.js"></script>
    <script src="~/Content/self-writing/js/Controller.js"></script>
    <script src="~/Content/download/angularjs/dirPagination.js"></script>
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="/TruongAnHCM/TAHCMHome/Index" class="logo logodaily" >
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini" style="color: white"><img src="~/Content/self-writing/Images/@Session["AVATAR"]+.png" class="user-image" alt="User Image" style="width:100%"></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg" style="color: white;">@Session["USERNAME"]</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" style="background: deepskyblue">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button"  style="color: white;">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Menu bar-->



                <!-- Menu TOP---------------------------------------------------------------------->
                <div class="navbar-custom-menu pull-right">
                    <ul class="nav navbar-nav ">
                       
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/Content/self-writing/Images/@Session["AVATAR"]" class="user-image" alt="User Image" style="width:50px; height: 50px;margin-top: -15px !important" >
                                <span class="hidden-xs">@Session["HO_VA_TEN"]</span>
                            </a>

                        </li>
                        <!-- Control Sidebar Toggle Button -->
                        <li><a href="/Home/Logout"><i class="fa fa-sign-out pull-right"></i> Log Out</a></li>
                    </ul>
                </div>
                <!--END Menu TOP------------------------------------------------------------------->
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar" style="background: #3C8DBC;color: white;">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">

                <!-- search form -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group" style="width: 100%">
                        <input id="searchtextbox" type="text" name="q" class="form-control"  placeholder="Search...">
                        <button type="submit" name="search" id="search-btn" class="btn btn-flat" style="display:inline-block;float:left;">
                            <i class="fa fa-search" id="searchtext"></i>
                        </button>
                    </div>
                </form>
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" style="background: #3C8DBC;color: white;">
                    <li class="header" style="background: #3C8DBC;color: white;"><h4>Menu</h4></li>
                    <li class="treeview">
                        <a href="/HangHoaDaiLy/Index">
                            <i class="fa fa-dashboard" style="font-size: 15px;color: white;"></i> <span style="color: white;">Products</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-pie-chart" style="font-size: 15px;color: white;"></i>
                            <span style="color: white;">Request for Price Quotation</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-users" style="font-size: 15px;color: white;"></i>
                            <span style="color: white;">Order</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                       
                    </li>

                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper"  style="background: none !important">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer" style="position: absolute;
    bottom: 0px;
    left: 0px;">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0
            </div>
            <strong>Copyright &copy; 2017 <a href="https://hoplongtech.com">hoplongtech.com</a>.</strong> All rights
            reserved.
        </footer>

        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- jQuery 2.2.3 -->
    <script src="~/Content/download/plugins/jQuery/jquery-2.2.3.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>

    <script src="~/Content/download/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/download/plugins/fastclick/fastclick.js"></script>
    <script src="~/Content/download/dist/js/app.min.js"></script>
    <script src="~/Content/self-writing/js/nav_hoplong_multilevel.js"></script>
</body>
</html>
