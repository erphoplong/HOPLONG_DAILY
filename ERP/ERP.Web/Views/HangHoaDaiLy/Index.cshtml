

@{
    ViewBag.Title = "Hàng hóa";
}
<meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
<link href="https://cdn.datatables.net/1.10.13/css/dataTables.bootstrap.min.css" rel="stylesheet" />

<link href="~/Content/self-writing/css/HangHoaDaiLy.css" rel="stylesheet" />
<link href="~/Content/download/dist/css/AdminLTE.css" rel="stylesheet" />
<link href="~/Content/download/bootstrap/css/layout_all.css" rel="stylesheet" />
<script src="~/Content/download/plugins/ckeditor/ckeditor.js"></script>
<script src="~/Content/download/plugins/jQuery/jquery-2.2.3.min.js"></script>
<div ng-app="hanghoaApp" ng-controller="hanghoaCtrl">
    <div class="content">
        

        <div id="quick-access">
            <form class="form-inline quick-search-form" role="form" action="#">
                <div class="form-group" style="width: 100%">
                    <input type="text" style="width: 60%" class="form-control" placeholder="Search" ng-model="search_text">
                </div>
            </form>
        </div>

        <br />
        
        <div class="modal fade" id="chitiethanghoa" role="dialog" tabindex="-1">
            <div class="modal-dialog modal-lg" style="width:80%;">
                <div class="modal-content" style="height: 600px;">

                    <div class="modal-body">
                        <div class="col-md-3">
                            <img ng-src="~/Content/self-writing/Images/{{item.MA_NHOM_HANG}}.png" style="width: 100%;height: 250px;" />
                        </div>
                        <div class="col-md-9">
                            <div style="margin-bottom: 5%;" class="col-md-12">
                                <h1>{{item.TEN_HANG | uppercase}}</h1>
                                <div class="col-md-3 detail">Mã hàng: {{item.MA_HANG}}</div>
                                <div class="col-md-2 detail">Hãng: {{item.MA_NHOM_HANG}}</div>
                                <div class="col-md-2 detail">Xuất xứ: {{item.XUAT_XU}}</div>
                            </div>
                            <div>
                                <ul class="nav nav-tabs">
                                    <li class="active"><a data-toggle="tab" href="#chitiet">Product Details</a></li>
                                    <li><a data-toggle="tab" href="#thongsokythuat">Thông số kỹ thuật</a></li>
                                    <li><a data-toggle="tab" href="#slton">Số lượng tồn</a></li>
                                </ul>
                                <div class="tab-content">
                                    <!--Tab chi tiết hàng hóa-->
                                    <div id="chitiet" class="tab-pane fade in active">

                                        <div class="col-md-6">
                                            <table class="table noborder">
                                                <tr>
                                                    <td>Unit</td>
                                                    <td>{{item.DON_VI_TINH}}</td>
                                                </tr>
                                                <tr>
                                                    <td>Khối lượng:</td>
                                                    <td></td>
                                                </tr>
                                                <tr>
                                                    <td>Giá bán:</td>
                                                    <td></td>
                                                </tr>
                                                <tr>
                                                    <td>Properties</td>
                                                    <td></td>
                                                </tr>
                                            </table>
                                        </div>
                                        <div class="col-md-6">
                                            <table class="table noborder">
                                                <tr>
                                                    <td>Quy cách đóng gói</td>
                                                    <td ng-bind-html="item.QUY_CACH_DONG_GOI | unsafe"></td>
                                                </tr>
                                                <tr>
                                                    <td>Sale Account</td>
                                                    <td></td>
                                                </tr>
                                                <tr>
                                                    <td>COGS Account</td>
                                                    <td></td>
                                                </tr>
                                                <tr>
                                                    <td>Note</td>
                                                    <td></td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                    <div id="slton" class="tab-pane fade">
                                        <div class="col-md-12">
                                            <table class="table noborder table-fill">
                                                <tr>
                                                    <td ng-repeat="tonkho in danhsachtonkho">{{tonkho.MA_KHO}}</td>
                                                </tr>
                                                <tr>
                                                    <td ng-repeat="tonkho in danhsachtonkho">{{tonkho.SL_TON}}</td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                    <div id="thongsokythuat" class="tab-pane fade">
                                        <p>- Mô tả chi tiết :</p>
                                        <p>- Điện áp :</p>
                                        <p>- Công suất :</p>
                                        <p>- Kích thước :</p>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>

            </div>
        </div>

        <input type="hidden" ng-model="userquantam" id="userquantam" value="@Session["USERNAME"]" />
